{% extends './base.html.twig' %}

{% block title %}RentalImages index{% endblock %}

{% block body %}
    <h1>RentalImages index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for rental_image in rental_images %}
            <tr>
                <td>{{ rental_image.id }}</td>
                <td>
                    <a href="{{ path('app_rental_images_show', {'id': rental_image.id}) }}">show</a>
                    <a href="{{ path('app_rental_images_edit', {'id': rental_image.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_rental_images_new') }}">Create new</a>
{% endblock %}
